--- Dockerfile	(date 1627810444635)
+++ Dockerfile	(date 1627810444635)
@@ -0,0 +1,22 @@
+FROM golang:alpine
+
+ENV GO111MODULE=on \
+    CGO_ENABLED=0 \
+    GOOS=linux \
+    GOARCH=amd64
+
+WORKDIR /build
+
+COPY go.mod .
+COPY go.sum .
+RUN go mod download
+
+COPY . .
+
+RUN go build ./cmd
+
+WORKDIR /dist
+
+RUN cp /build/cmd .
+
+CMD ["/dist/cmd"]
